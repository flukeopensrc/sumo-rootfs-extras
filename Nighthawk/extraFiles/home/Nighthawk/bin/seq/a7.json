    "A7_INIT": {
        "description": "Initialize the MSP430 on the A7",
        "steps": [
            {
                "description": "SPI on bits 4 & 5, PWM on bit 6; all else GPIO",
                "operation": "write",
                "register": "P7P1SEL",
                "value": "0x70"
            },
            {
                "description": "Bit 0 is input; bit 4 is SOMI; rest are outputs",
                "operation": "write",
                "register": "P7P1DIR",
                "value": "0xee"
            },
            {
                "description": "Set whole port 2 as output except Bit 4",
                "operation": "write",
                "register": "P7P2DIR",
                "value": "0xef"
            },
            {
                "description": "Set all of port 3 to be outputs",
                "operation": "write",
                "register": "P7P3DIR",
                "value": "0xff"
            },
            {
                "description": "Set all of port 5 to be outputs",
                "operation": "write",
                "register": "P7P5DIR",
                "value": "0xff"
            },
            {
                "description": "Set all of port 6 to be outputs",
                "operation": "write",
                "register": "P7P6DIR",
                "value": "0xff"
            },
            {
                "description": "Set all of port 7 to be outputs",
                "operation": "write",
                "register": "P7P7DIR",
                "value": "0xff"
            },
            {
                "description": "Set all of port 8 to be outputs",
                "operation": "write",
                "register": "P7P8DIR",
                "value": "0xff"
            },
            {
                "description": "Configure PWM2 to power the inverter used in the DC power function",
                "operation": "write",
                "register": "P7PWM2CTL",
                "value": "0x0211"
            },
            {
                "description": "",
                "operation": "write",
                "register": "P7PWM2EX0",
                "value": "0x0000"
            },
            {
                "description": "",
                "operation": "write",
                "register": "P7PWM20CCTL",
                "value": "0x0001"
            },
            {
                "description": "",
                "operation": "write",
                "register": "P7PWM21CCTL",
                "value": "0x04e1"
            },
            {
                "description": "",
                "operation": "write",
                "register": "P7PWM20",
                "value": "0x6000"
            },
            {
                "description": "",
                "operation": "write",
                "register": "P7PWM21",
                "value": "0x3000"
            },
            {
                "description": "Put port pins and relays in dormant state.",
                "operation": "exec",
                "sequence": "A7_DORM"
            },
            {
                "description": "Harmless delay with no comma at the end",
                "operation": "delay",
                "value": "0x05"
            }
        ]
    },
