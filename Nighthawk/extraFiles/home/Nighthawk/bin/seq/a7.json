{
    "A7_DORM": {
        "description": "Put the A7 in a dormant state",
        "steps": [
            {
                "description": "P2 Dormant state",
                "operation": "write",
                "register": "P7P2",
                "value": "0xdb"
            },
            {
                "description": "P6 Dormant state",
                "operation": "write",
                "register": "P7P6",
                "value": "0xe0"
            },
            {
                "description": "P7 Dormant state",
                "operation": "write",
                "register": "P7P7",
                "value": "0xf1"
            },
            {
                "description": "P8 Dormant state",
                "operation": "write",
                "register": "P7P8",
                "value": "0xf0"
            },
            {
                "description": "Strobe the latching relays",
                "operation": "exec",
                "sequence": "A7_STROBE"
            },
            {
                "description": "Disable the MSP430 signal to the DCP inverter",
                "operation": "exec",
                "sequence": "A7_DISABLE_DCP_INVERTER"
            },
            {
                "description": "Harmless delay with no comma at the end",
                "operation": "delay",
                "value": "0x05"
            }
        ]
    },
    "A7_DORM_REMOVED": {
        "description": "Holder for disabling parts of A7_DORM; chasing an elusive power up issue",
        "steps": [
            {
                "description": "P3 Dormant state",
                "operation": "write",
                "register": "P7P3",
                "value": "0xbf"
            },
            {
                "description": "P5 Dormant state",
                "operation": "write",
                "register": "P7P5",
                "value": "0x00"
            },
            {
                "description": "Harmless delay with no comma at the end",
                "operation": "delay",
                "value": "0x05"
            }
        ]
    },
    "A7_STROBE": {
        "description": "Strobe the A7 latching relays.",
        "steps": [
            {
                "description": "Set relay strobe low",
                "mask": "0x20",
                "operation": "modify",
                "register": "P7P6",
                "value": "0x00"
            },
            {
                "description": "",
                "operation": "delay",
                "value": "0x10"
            },
            {
                "description": "Set relay strobe high",
                "mask": "0x20",
                "operation": "modify",
                "register": "P7P6",
                "value": "0x20"
            }
        ]
    },
