    "A7_CLEAR_CHECK_OVERVOLTAGE": {
        "description": "Clear protection and recheck the overvoltage bit",
        "steps": [
            {
                "description": "Clear overvoltage bit",
                "operation": "modify",
                "register": "P7P2",
                "mask": "0x01",
                "value": "0x00"
            },
            {
                "description": "",
                "operation": "delay",
                "value": "0x05"
            },
            {
                "description": "End of clearing the bit",
                "operation": "modify",
                "register": "P7P2",
                "mask": "0x01",
                "value": "0x01"
            },
            {
                "description": "Wait to see if the bit reasserts itself",
                "operation": "delay",
                "value": "0x05"
            },
            {
                "description": "Recheck the fault bit",
                "operation": "read",
                "register": "P7P2"
            }
        ]
    },
