    "A6_SET_POLARITY_NEGATIVE": {
        "description": "Set polarity negative (assume it is positive now).",
        "steps": [
            {
                "description": "Set the DAC close to zero for positive polarity",
                "operation": "write",
                "register": "A6VDACCOARSE",
                "value": "0x138"
            },
            {
                "description": "Set the DAC close to zero for positive polarity",
                "operation": "write",
                "register": "A6VCALCOARSE",
                "value": "0x138"
            },
            {
                "description": "Switch A6 output from DAC to DDS and bypass feedback capacitor",
                "mask": "0x28",
                "operation": "modify",
                "register": "A6DDSEL1",
                "value": "0x00"
            },
            {
                "description": "Set polarity relay to negative.",
                "mask": "0x08",
                "operation": "modify",
                "register": "A6DDSRLY2",
                "value": "0x00"
            },
            {
                "description": "",
                "operation": "exec",
                "sequence": "A6_STROBERELAYS"
            },
            {
                "description": "Set DAC close to zero for negative polarity",
                "operation": "write",
                "register": "A6VDACCOARSE",
                "value": "0x3e9c"
            },
            {
                "description": "Set the DAC close to zero for positive polarity",
                "operation": "write",
                "register": "A6VCALCOARSE",
                "value": "0x3e9c"
            },
            {
                "description": "Reconnect feedback capacitor",
                "mask": "0x08",
                "operation": "modify",
                "register": "A6DDSEL1",
                "value": "0x08"
            },
            {
                "description": "A little delay (TODO: tweak value or eliminate based on testing)",
                "operation": "delay",
                "value": "0x10"
            },
            {
                "description": "Switch output back to DAC",
                "mask": "0x20",
                "operation": "modify",
                "register": "A6DDSEL1",
                "value": "0x20"
            }
        ]
    },
