    "A5_INIT": {
        "description": "Initialize the a5 msp430",
        "steps": [
            {
                "description": "enable spi, ADC ch0-2",
                "operation": "force",
                "register": "P5P1SEL",
                "value": "0x3f"
            },
            {
                "description": "set data bus as output",
                "operation": "force",
                "register": "P5P5DIR",
                "value": "0xff"
            },
            {
                "description": "",
                "operation": "force",
                "register": "P5P5",
                "value": "0x00"
            },
            {
                "description": "",
                "operation": "force",
                "register": "P5P6DIR",
                "value": "0xf7"
            },
            {
                "description": "",
                "operation": "force",
                "register": "P5P6",
                "value": "0xf0"
            },
            {
                "description": "",
                "operation": "force",
                "register": "P5P7DIR",
                "value": "0x3f"
            },
            {
                "description": "",
                "operation": "force",
                "register": "P5P7",
                "value": "0x3b"
            },
            {
                "description": "",
                "operation": "force",
                "register": "P5P8DIR",
                "value": "0xff"
            },
            {
                "description": "",
                "operation": "force",
                "register": "P5P8",
                "value": "0xff"
            },
            {
                "description": "enable ADC ch3-5",
                "operation": "force",
                "register": "P5P9SEL",
                "value": "0x0e"
            },
            {
                "description": "MSB first, master mode, synchronous, SMCLK",
                "operation": "force",
                "register": "P5SPICTL",
                "value": "0xa981"
            },
            {
                "description": "prescaler of 16; 1MHz clock?",
                "operation": "force",
                "register": "P5SPIBRW",
                "value": "0x0010"
            },
            {
                "description": "",
                "operation": "force",
                "register": "P5PWM1CTL",
                "value": "0x0250"
            },
            {
                "description": "",
                "operation": "force",
                "register": "P5PWM1EX0",
                "value": "0x0000"
            },
            {
                "description": "",
                "operation": "force",
                "register": "P5PWM10CCTL",
                "value": "0x0000"
            },
            {
                "description": "",
                "operation": "force",
                "register": "P5PWM11CCTL",
                "value": "0x00e0"
            },
            {
                "description": "16-cycle sample-and-hold time, ADC on",
                "operation": "force",
                "register": "P5ADCTL0",
                "value": "0x0210"
            },
            {
                "description": "",
                "operation": "force",
                "register": "P5ADCTL1",
                "value": "0x0000"
            },
            {
                "description": "set 10-bit resolution",
                "operation": "force",
                "register": "P5ADCTL2",
                "value": "0x0010"
            },
            {
                "description": "select refs: V(R+)=VREF, V(R-)=AVSS",
                "operation": "force",
                "register": "P5ADMCTL",
                "value": "0xa1"
            },
            {
                "description": "avearge 16 readings",
                "operation": "force",
                "register": "P5ADAVG",
                "value": "0x0010"
            },
            {
                "description": "now initialize all of the register data",
                "operation": "exec",
                "sequence": "A5_DORM"
            }
        ]
    }
}
