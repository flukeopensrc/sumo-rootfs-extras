#!/bin/sh

export QMLSCENE_DEVICE=softwarecontext
export QT_QPA_PLATFORM=linuxfb
export QT_QPA_FONTDIR=/usr/share/fonts/ttf

cd /home/Nighthawk/bin
/home/Nighthawk/bin/subpub &> /dev/null 2>&1 &
/home/Nighthawk/bin/errorpub &> /dev/null 2>&1 &
sleep 1
/home/Nighthawk/bin/settings &> /dev/null 2>&1 &
/home/Nighthawk/bin/cadmus &> /dev/null 2>&1 &
/home/Nighthawk/bin/opman &> /dev/null 2>&1 &
sleep 12
/home/Nighthawk/bin/procedure &> /dev/null 2>&1 &
/home/Nighthawk/bin/state &> /dev/null 2>&1 &
/home/Nighthawk/bin/remoteapp &> /dev/null 2>&1 &
/home/Nighthawk/bin/start-ui
