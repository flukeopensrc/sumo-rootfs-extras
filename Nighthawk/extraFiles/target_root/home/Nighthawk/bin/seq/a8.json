    "A8_DCV_GT330V_SETUP": {
        "description": "Set up A8 for values from 330V to 1020V",
        "steps": [
            {
                "description": "Set up A8 relays",
                "operation": "exec",
                "sequence": "A8_1000V_STBY"
            },
            {
                "description": "Select 1000V supply",
                "operation": "exec",
                "sequence": "A8_SUPPLY_HVDC_ON"
            }
        ]
    },
