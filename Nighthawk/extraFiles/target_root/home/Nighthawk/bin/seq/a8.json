    "A8_DORM": {
        "description": "Put the A8 into known safe state",
        "steps": [
            {
                "description": "Turn off HV supplies",
                "operation": "exec",
                "sequence": "A8_SUPPLY_ALL_OFF"
            },
            {
                "description": "Reset A8 relays",
                "operation": "write",
                "register": "A8RLY",
                "value": "0x000"
            }
        ]
    },
