{
    "A14_A6DAC_CAL": {
        "description": "Set up for VDAC/IDAC ratio and zero cal",
        "steps": [
            {
                "description": "",
                "operation": "write",
                "register": "A14RLY1",
                "value": "0x000"
            },
            {
                "description": "",
                "operation": "write",
                "register": "A14RLY2",
                "value": "0x012"
            }
        ]
    },
    "A14_ADSD0": {
        "description": "Measure sigma-delta A/D 0 for zero cal",
        "steps": [
            {
                "description": "Average 2000 measurements",
                "operation": "write",
                "register": "P14SD0AVG",
                "value": "0x7D0"
            },
            {
                "description": "Start the A/D",
                "operation": "force",
                "register": "P14SD0",
                "value": "0x0"
            },
            {
                "description": "Wait this long for A/D to finish",
                "operation": "delay",
                "value": "0x4B0"
            },
            {
                "description": "Read the A/D",
                "operation": "read",
                "register": "P14SD0"
            }
        ]
    },
    "A14_DORM": {
        "description": "Set A14 to dormant.",
        "steps": [
            {
                "description": "Run the rest of the dormant sequence",
                "operation": "exec",
                "sequence": "A14_DORM_NO_LED"
            },
            {
                "description": "Terminal rings for DCV, standby light on",
                "operation": "write",
                "register": "A14LED",
                "value": "0x0052"
            }
        ]
    },
    "A14_DORM_NO_LED": {
        "description": "Set A14 to dormant without changing LEDs.",
        "steps": [
            {
                "description": "Set TC RJ_MSR* hi",
                "operation": "write",
                "register": "P14P2",
                "value": "0x40"
            },
            {
                "description": "Set all other TC control bits hi",
                "operation": "write",
                "register": "P14P3",
                "value": "0xfc"
            },
            {
                "description": "Set all relays to dorm, part 1",
                "operation": "write",
                "register": "A14RLY1",
                "value": "0x000"
            },
            {
                "description": "Set all relays to dorm, part 2",
                "operation": "write",
                "register": "A14RLY2",
                "value": "0x002"
            },
            {
                "description": "All LED's off, standby light on",
                "operation": "write",
                "register": "A14LED",
                "value": "0x0002"
            }
        ]
    },
    "A14_DUMP": {
        "description": "Get A14RLY1, A14RLY2, A14LED",
        "steps": [
            {
                "description": "",
