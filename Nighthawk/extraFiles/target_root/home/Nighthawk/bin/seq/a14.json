    "A14_FUNC_TC_MEAS": {
        "description": "Thermocouple measure setup",
        "steps": [
            {
                "description": "Enable TC_MSR*",
                "operation": "write",
                "register": "P14P3",
                "value": "0xBF"
            },
            {
                "description": "'operate' sequence for TC functions",
                "operation": "exec",
                "sequence": "A14_FUNC_TC_OPER"
            }
        ]
    },
    "A14_FUNC_TC_OPER": {
        "description": "Thermocouple measure setup",
        "steps": [
            {
                "description": "Internal guard enabled",
                "operation": "write",
                "register": "A14RLY2",
                "value": "0x01A"
            },
            {
                "description": "TC relay energized",
                "operation": "force",
                "register": "A14RLY1",
                "value": "0x100"
            },
            {
                "description": "Operate and TC socket LED enabled",
                "operation": "write",
                "register": "A14LED",
                "value": "0x4001"
            }
        ]
    },
    "A14_HII_DUAL_OPER": {
        "description": "Used for dual output ranges (>3A AC/DC)",
        "steps": [
            {
                "description": "Turn operate LED on, standby off",
                "operation": "exec",
                "sequence": "A14_LED_OPER"
            },
            {
                "description": "Connect terminals, leaving internal short in place",
                "mask": "0x1E0",
                "operation": "modify",
                "register": "A14RLY2",
                "value": "0x1C0"
            },
            {
                "description": "Disconnect internal short",
                "mask": "0x1E0",
                "operation": "modify",
                "register": "A14RLY2",
                "value": "0x0C0"
            }
        ]
    },
    "A14_HII_DUAL_STBY": {
        "description": "Used for dual output ranges (>3A AC/DC)",
        "steps": [
            {
                "description": "Disconnect low current high terminal just in case.",
                "mask": "0x080",
                "operation": "modify",
                "register": "A14RLY1",
                "value": "0x000"
            },
            {
                "description": "connect internal short, leaving terminals as they were",
                "mask": "0x100",
                "operation": "modify",
                "register": "A14RLY2",
                "value": "0x100"
            },
            {
                "description": "disconnect terminals",
                "mask": "0x1E0",
                "operation": "modify",
                "register": "A14RLY2",
                "value": "0x100"
            },
            {
                "description": "Set up current terminal rings",
                "mask": "0x0E03",
                "operation": "modify",
                "register": "A14LED",
                "value": "0x0A02"
            }
        ]
    },
